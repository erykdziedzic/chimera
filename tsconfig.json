{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "typeRoots": ["./node_modules/@types", "./custom_typings"],
    "experimentalDecorators": true,
  },
  "include": [
    "./src/**/*", "custom_typings"
  ],
  "exclude": [
    "node_modules"
  ]
}
